{
	"python.analysis.diagnosticSeverityOverrides": {
		"reportGeneralTypeIssues": "none"
	},
	"python.linting.enabled": true,
	"python.linting.mypyEnabled": true,
	"python.linting.pylintArgs": ["--disable=wildcard-import,invalid-name,missing-function-docstring,broad-except,logging-format-interpolation,missing-module-docstring,missing-class-docstring,line-too-long,too-few-public-methods,inconsistent-return-statements,too-many-branches,too-many-locals,too-many-statements,too-many-arguments,too-many-instance-attributes", "--enable=unused-import"],
	"suggest.enablePreview": true
}
